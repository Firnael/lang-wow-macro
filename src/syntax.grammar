@top Program { expression* }

@skip { space }

expression {
  Function { "/" Identifier } |
  Condition { "[" Identifier "]" } |
  Identifier |
  String |
  Boolean |
  Application { "(" expression* ")" }
}

@tokens {
  Identifier { @asciiLowercase+ }

  String { '"' (!["\\] | "\\" _)* '"' }

  Boolean { "#t" | "#f" }

  space { $[ \t\n\r]+ }

  "(" ")"
}

@detectDelim
