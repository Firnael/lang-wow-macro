@top Program { Line* }

Line { (ShowTooltip | Function expression*) NewLine?}

expression {
  Condition |
  ConditionOperator |
  Name |
  Id
}

@tokens {
  ShowTooltip { "#showtooltip" }

  Name { @asciiLetter+ }

  Id { @digit+ }

  Function { '/' @asciiLowercase+ }

  Condition { '[' '@'? @asciiLowercase+ ']' }

  ConditionOperator { ';' }

  NewLine { ('\n' | '\r' | '\n\r')+ }

  space { $[ \t]+ }
}

@skip { space }
@detectDelim
