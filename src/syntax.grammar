@top Program { Line* }

Line { (ShowTooltip | Function expression*) NewLine?}

expression {
  Condition |
  ConditionOperator |
  Identifier
}

@tokens {
  ShowTooltip { "#showtooltip" }

  Identifier { @asciiLetter+ }

  Function { '/' @asciiLowercase+ }

  Condition { '[' '@'? @asciiLowercase+ ']' }

  ConditionOperator { ';' }

  NewLine { ('\n' | '\r' | '\n\r')+ }

  space { $[ \t]+ }
}

@skip { space }
@detectDelim
