@top Program { expression* }

@skip { space }

expression {
  Function |
  Condition |
  Identifier |
  String |
  Boolean |
  Application { "(" expression* ")" }
}

@tokens {
  Identifier { @asciiLowercase+ }

  Function { '/' @asciiLowercase+ }

  Condition { '[' @asciiLowercase+ ']' }

  String { '"' (!["\\] | "\\" _)* '"' }

  Boolean { "#t" | "#f" }

  space { $[ \t\n\r]+ }

  "(" ")"
}

@detectDelim
