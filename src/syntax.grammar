@top Program { expression* }

@skip { space }

expression {
  ShowTooltip |
  Function |
  Condition |
  Identifier |
  String |
  Application { "(" expression* ")" }
}

@tokens {
  ShowTooltip { '#showtooltip' }

  Identifier { @asciiLetter+ }

  Function { '/' @asciiLowercase+ }

  Condition { '[' @asciiLowercase+ ']' }

  String { '"' (!["\\] | "\\" _)* '"' }

  space { $[ \t\n\r]+ }
}

@detectDelim
